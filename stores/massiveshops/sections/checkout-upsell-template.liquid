{% assign relatedProductHandle = 'cute-cat-ceramic-mug' %}
{% assign product = all_products [relatedProductHandle]%}

{%- assign product_thumb_size = '160x160' -%}
{%- assign product_image_zoom_size = '1024x1024' -%}
{%- assign current_variant = product.selected_or_first_available_variant -%}
{%- assign discount_percentage = product.compare_at_price_max | minus: product.price | times: 100.0 | divided_by: product.compare_at_price_max | money_without_currency | times: 100 | remove: '.0' -%}

{% comment %}Items not displayed to get variants information to handle pics and videos{% endcomment %}



<div style="display: none" id="media_info">
    {% if product.images.size > 1 %}
        {% for image in product.images %}
            <div data-src-featured-image="{{ image | product_img_url: 'master' }}" data-alt="{{ image.alt | escape }}"></div>
        {% endfor %}
    {% endif %}
  	<form class="search" action="/search">
        <input type="text" placeholder="Search" name="q" value="{{ search.terms | escape }}"  />
        <input type="submit" value="Search" />
    </form>
</div>

<div id="product-box-upsell" class="product-box-upsell" data-section-id="{{ section.id }}">
    <div class="container">

        {% comment %}
        <div class="upsell-title">
            <h3>For a Limited Time FREE Shipping</h3>
        </div>
        {% endcomment %}

        <div class="upsell-subtitle text-center">
            <div class="fn-promo-upsell">
                <span class="fn-promoCounterTitle">
                  	{% if product.compare_at_price_max > product.price %}
                    Time remaining to get <span class=""><b>{{ discount_percentage }}% OFF</b></span>:&nbsp;
                  	{% else %}
                  	Offer Ends In:
                  	{% endif %}
                </span>
                <div class="ce-countdown-wrapper fn-countdown-upsell">
                    <div class="ce-countdown ce-countdown--theme-7">
                        <strong>
                            <span class="ce-digits ce-days"></span>
                            <span>:</span>
                            <span class="ce-digits ce-hours"></span>
                            <span>:</span>
                            <span class="ce-digits ce-minutes"></span>
                            <span>:</span>
                            <span class="ce-digits ce-seconds"></span>
                        </strong>
                    </div>
                </div>
            </div>
        </div>

        <div class="product-box" id="product-box" data-section-id="{{ section.id }}">
            <div class="row">

                <div class="col-lg-8 col-md-8">
                  	<div class="product-images">
                    	<!-- The first image pulled from the product is a large image (480x480), subsequent images are "compact" which is 160x160 -->
                    	<div id="product_images">
                          	{% if product.images.size > 1 %}
                          	<ul id="product-thumbnails" class="col-md-2 col-sm-2">
                              	{% for image in product.images %}
                              	<li class="col-md-12 thumb row{% cycle ' alpha clearcolumn', '', '', ' omega' %}">
                                  	<a {% comment %}href="{{ image | product_img_url: 'original' }}"{% endcomment %} data-src-large="{{ image | product_img_url: 'large'}}" data-src-master="{{ image | product_img_url: 'master'}}" title="{{ image.alt | escape }}" class="image-swap">
                                      	<img src="{{ image | product_img_url: 'medium'}}" alt="{{ image.alt | escape }}" />
                                  	</a>
                              	</li>
                              	{% endfor %}
                          	</ul>
                          	{% endif %}

                      		{% for image in product.images %}
                      		{% if forloop.first %}
                      		{% assign featured_image = product.selected_or_first_available_variant.featured_image | default: product.featured_image %}
                      		<div class="featured-image-div col-md-8 col-md-offset-1 col-sm-9 col-sm-offset-1 row" {% if section.settings.enable_zoom %}data-zoom="true"{% endif %}>
                              	<div id="variant-inventory">
                                  	Only <span class="number">5</span> in stock!
                              	</div>
		                        <img title="{{ image.alt }}" id="FeaturedImage-{{ section.id }}" src="{{ featured_image | img_url: 'master' }}" alt="{{ featured_image.alt | escape }}" class="product-main-image" />
                              	<div class="hidden" id="featured-image-loader">
                                  	<i class="fa fa-circle-o-notch fa-spin fa-3x fa-fw"></i>
                                  	<span class="sr-only">Loading...</span>
                              	</div>
                          	</div>
                          	{% endif %}
                          	{% endfor %}
                      	</div><!-- Product-images -->
                  	</div><!-- col-12 -->
                  	<div id="mobile-product-images" class="visible-xs">
                        <div class="siema-wrapper flexslider-container col-md-12 mobile-product-slider-container">
                            <div class=" mobile-product-slider">
                                {% assign carouselItemsCounter = 0 %}
                                {% for image in product.images %}

                                <div class="item {% if forloop.first == true %}active{% endif %}" data-index="{% increment carouselItemsCounter %}">
                                    {% assign product_video =  image.alt | escape %}

                                    {% assign product_video_url_noextension =  product_video | replace: '.mp4', '' %}
                                    {% assign product_video_url_noextension =  product_video_url_noextension | replace: '.ogg', '' %}
                                    {% assign product_video_url_noextension =  product_video_url_noextension | replace: '.ogv', '' %}
                                    {% assign product_video_url_noextension =  product_video_url_noextension | replace: '.webm', '' %}

                                    {% if product_video contains 'youtube' %}
                                    {{ image.alt }}
                                    {% elsif product_video contains 'https' %}
                                    <div class="video-wrapper" onclick="playPause(this)">
                                        <video loop class="video" poster="{{ image | product_img_url: 'large'}}" autoplay playsinline preload="auto"{% comment %}onclick="playPause(this)"{% endcomment %} muted>
                                            <source src="{{ product_video_url_noextension }}.mp4" type="video/mp4">
                                            <source src="{{ product_video_url_noextension }}.webm" type="video/webm">
                                            <source src="{{ product_video_url_noextension }}.ogv" type="video/ogg">
                                            <source src="{{ product_video_url_noextension }}.ogg" type="video/ogg">
                                        </video>

                                        <div class="video-overlay" style="display: none"></div>
                                        <div class="video-loading"><div class="bg"></div><span class="text">loading video...</span></div>

                                    </div>
                                    {% else %}
                                    <img src="{{ image | product_img_url: 'grande'}}" alt="{{ image.alt | escape }}" class="img-responsive" />
                                    {% endif %}
                                </div>
                                {% endfor %}
                            </div>
                            <div class="siema-nav">
                                <button class="siema-prev">&lt;</button>
                                <button class="siema-next">&gt;</button>
                            </div>
                        </div>
                    </div>
              	</div><!-- col-8 -->
              	<!-- Product Description -->
              	<div class="col-xs-12 col-md-4">
                	<h1 class="product-description-header">{{ product.title }}</h1>
                    {% if product.available %}
                    <form action="/cart/add" method="post" id="add-to-cart-form">
                        <div class="row">
                            <div class="col-md-12">

                              	<div class="prices-apps-wrapper clearfix">
                                    <div class="price-wrapper text-center">
                                        <h5 class="previous-price compare-at-price">{{ current_variant.compare_at_price | money }}</h5>

                                        {% if current_variant.compare_at_price > current_variant.price %}
                                        <h3 id="price-field" class="current-price"><span class="money">{{ current_variant.price | money }}</span></h3>

                                        <div class="save">
                                          	<h3>SAVE {{ current_variant.compare_at_price | minus: current_variant.price  | money }}</h3>
                                        </div>
                                        {% endif %}
                                    </div>
                                </div>
                                <!-- MAIN SELECTORS -->
                                {% if product.variants.size > 0 %}
                                {% for option in product.options_with_values %}
                                {% capture optionName %}{{ option.name | handleize }}{% endcapture %}
                                {% if product.variants.size > 1 %}

                                <div class="variant-select-selects" style="display: none;" >
                                    <div class="selector-wrapper {{option.name | downcase}} clearfix">
                                        <span class="label">{{ option.name }}:</span>
                                        <div class="default-selector" >
                                            <select class="form-control single-option-selector" data-variant-type="{{ optionName }}">
                                                {% for value in option.values %}
                                                <option value="{{value}}"{% if option.selected_value == value %}selected{% endif %}>{{ value }}</option>
                                                {% endfor %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                </div>

                                <div class="variant-select-buttons" >

                                    <div class="selector-wrapper {{option.name | downcase}} custom-radios clearfix">
                                        <span class="label">{{ option.name }}:</span>
                                        <div class="custom-radios-wrapper">
                                            <div class="flex">
                                                {% for value in option.values %}
                                                <input
                                                       {% if option.selected_value == value %}checked{% endif %}
                                                       name="product-{{ optionName }}"
                                                       type="radio"
                                                       id="{{ optionName }}-radio-{{ forloop.index0 }}"
                                                       value="{{value}}"
                                                       class="fn-radio"
                                                       data-variant-type="{{ optionName }}"
                                                       />
                                                <label for="{{ optionName }}-radio-{{ forloop.index0}}"
                                                       class="
                                                              {% if option.selected_value == value %}labelChecked{% endif %}
                                                              "
                                                       data-value="{{ value }}"
                                                       >
                                                  {{ value }}
                                                </label>
                                                {% endfor %}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="clearfix"></div>
                                </div>
                                {% endif %}
                                {% endfor %}
                                {% endif %}

                                <!-- MAIN HIDDEN SELECT -->
                                <select name="id" id="ProductSelect-{{ section.id }}" data-section="{{ section.id }}" class="form-control original-select product-form__variants no-js" style="display: none;">
                                    {% for variant in product.variants %}
                                    {% if variant.available %}
                                    <option {% if variant == current_variant %} selected="selected" {% endif %} value="{{ variant.id }}">
                                        {{ variant.title }} - <span class="money">{{ variant.price | money }}</span>
                                    </option>
                                    {% else %}
                                    <option disabled="disabled">{{ variant.title }} - {{ 'products.product.sold_out' | t }}</option>
                                    {% endif %}
                                    {% endfor %}
                                </select>

                                <div class="selector-wrapper quantity">
                                    <span class="label">{{ 'products.product.quantity' | t }}:</span>

                                    <div class="quantity-wrapper">
                                        <div id="quantity-control-minus" class="quantity-control minus">-</div>
                                        <input type="number" id="Quantity" name="quantity" value="1" min="1" class="quantity-selector">
                                        <div id="quantity-control-plus" class="quantity-control plus">+</div>
                                    </div>
                                    <div class="clear"></div>
                                </div>
                            </div>
                            <div class="clearfix"></div>

                            <div class="col-md-12">
                                <button type="submit" name="add" id="purchase" class="btn btn-success custom-font"
                                        data-use-ajax="true">
                                    BUY NOW
                                </button>
                            </div>

                          	<div class="col-md-12">
                              	<button class="btn-dismiss">No, thanks. Continue to complete my order.</button>
                            </div>
                        </div>
                    </form>
                    {% else %}
                    <div class="col-md-12 col-sm-12 col-xs-12">
