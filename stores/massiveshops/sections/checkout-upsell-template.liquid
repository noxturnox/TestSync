<script>
  $('.selector-wrapper select, .fn-radio, .fn-selector').on('change', function() {
      var jQEl = $(this);
      var variantType = jQEl.attr('data-variant-type');
      var mainSelect = $('.selector-wrapper select[data-variant-type="' + variantType + '"]');
      var selects = $('select.fn-selector[data-variant-type="' + variantType + '"]');
      var radio = $('input[type="radio"][data-variant-type="' + variantType + '"][value="' + jQEl.val() + '"]');

      radio.parent().find('.labelChecked').removeClass('labelChecked');

      if (!radio.is(':checked')) {
          var label = $('label[for="' + radio.attr('id') + '"]');
          label.click();
      }

      var selectsToUpdate = selects.filter(function() {
          return $(this).val() !== jQEl.val();
      });

      var mainToUpdate = mainSelect.filter(function() {
          return $(this).val() !== jQEl.val();
      });

      selectsToUpdate.find('option').removeAttr('selected');
      selectsToUpdate.filter(function() {
          return $(this).val() === jQEl.val()
      }).attr('selected', true);

      mainToUpdate.val(jQEl.val()).change();
      selectsToUpdate.val(jQEl.val());
  });
</script>

